{
  "compilerOptions": {
    "target": "es6",
    "module": "es2020",
    "outDir": "../../public/JS",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "removeComments": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}